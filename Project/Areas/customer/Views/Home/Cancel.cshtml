@model IEnumerable<Project_Model.Order>
@{
    ViewData["Title"] = "Cancel";
}

<div class="row">
 <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>

                <th>orderTotal</th>
                <th>Date</th>
                <th>PhoneNumber</th>+
                <th>state</th>
                <th>City</th>
                <th>pay</th>

            </tr>
        </thead>
        @foreach (var Item in Model)
        {
            <tbody>
                <tr>

                    <td>@Item.Id</td>
                    <td>@Item.Name</td>

                    <td>@Item.orderTotal</td>
                    <th>@Item.OrderData</th>
                    <td>@Item.PhoneNumber</td>
                    <td>@Item.state</td>
                    <td>@Item.City</td>
                    <td>@Item.OrderStatus</td>
                    <td>
                      @*  <a href="@Url.Action("Details", "Home", new { id = love.Id })">Details</a>
                        <a href="@Url.Action("Cancel", "Home", new { id = love.Id })">Details</a>
*@
                    </td>

                </tr>
            </tbody>
        }



    </table>
</div>